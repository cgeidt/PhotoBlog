<li class="list-group-item active">	  
  <h4 class="list-group-item-heading"><%= photo.title %></h4>
  <p class="list-group-item-text">
    <%= photo.description %>
    <br/>
    <%= photo.url %>
  </p>

  <p>
  <%= link_to 'Delete photo', [photo.album, photo],
  	 class: 'btn btn-danger',
     method: :delete,
     data: { confirm: 'Are you sure?' } %>
	</p>
</li>